<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Control colors.-->
    <SolidColorBrush x:Key="窗口背景色" Color="#FF99CCFF"/>
    <!--Border colors-->    
    <SolidColorBrush x:Key="窗口背景深色"
                     Color="#FF0099FF"/>
    <SolidColorBrush x:Key="分隔条颜色"
                     Color="#FF0099FF"/>
    <SolidColorBrush x:Key="边框颜色浅" Color="#FF0099FF"/>
    <SolidColorBrush x:Key="边框颜色" Color="#FF669999"/>
    <SolidColorBrush x:Key="字体颜色浅" Color="#FF0099FF"/>
    <SolidColorBrush x:Key="字体颜色标准" Color="#FF101010"/>
    <SolidColorBrush x:Key="屏幕颜色" Color="#FF666666"/>
    <SolidColorBrush x:Key="表行颜色" Color="White"/>
    <SolidColorBrush x:Key="表行颜色1" Color="#FFABCDEF"/>
    <SolidColorBrush x:Key="工作状态颜色" Color="#FF0099FF"/>
    <LinearGradientBrush x:Key="线性渐变颜色"
        EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FF0099FF"
                                      Offset="0"/>
                                        <GradientStop Color="#FF99CCFF" 
                                      Offset="1"/>
                                    </LinearGradientBrush>
    <SolidColorBrush x:Key="控件有效" Color="#FFABCDEF"/>
    <SolidColorBrush x:Key="窗口背景深色半透明"
                     Color="#FF0099FF" Opacity="0.6"/>
</ResourceDictionary>