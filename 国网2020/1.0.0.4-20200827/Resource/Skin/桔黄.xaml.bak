<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Control colors.-->
    <SolidColorBrush x:Key="窗口背景色"
                     Color="#FFFFFF66"/>
    <!--Border colors-->    
    <SolidColorBrush x:Key="窗口背景深色" Color="#FFFF6600" /> 
    <SolidColorBrush x:Key="分隔条颜色"
                     Color="#FFFF6600"/>
    <SolidColorBrush x:Key="边框颜色浅" Color="White"/>
    <SolidColorBrush x:Key="边框颜色" Color="#FF999999"/>
    <SolidColorBrush x:Key="字体颜色浅" Color="#FF333366"/>
    <SolidColorBrush x:Key="字体颜色标准" Color="#FF330033"/>
    <SolidColorBrush x:Key="屏幕颜色" Color="#FF666666"/>
    <SolidColorBrush x:Key="表行颜色" Color="#FFFFFFCC"/>
    <SolidColorBrush x:Key="表行颜色1" Color="#FFFF9933"/>
    <SolidColorBrush x:Key="工作状态颜色" Color="#FFFF9933"/>  
                     <LinearGradientBrush x:Key="线性渐变颜色"
        EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFFFFF66"
                                      Offset="0"/>
                                        <GradientStop Color="#FFFF6600" 
                                      Offset="1"/>
                                    </LinearGradientBrush>
    <SolidColorBrush x:Key="控件有效" Color="#FFFF9933"/>
    <SolidColorBrush x:Key="窗口背景深色半透明"
                     Color="#FFFF6600" Opacity="0.6"/>
</ResourceDictionary>