<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Control colors.-->
    <SolidColorBrush x:Key="窗口背景色" Color="#FFFFCCCC"/>
    <!--Border colors-->    
    <SolidColorBrush x:Key="窗口背景深色"
                     Color="#FFFF99CC"/>
    <SolidColorBrush x:Key="分隔条颜色"
                     Color="#FFFF99CC"/>
    <SolidColorBrush x:Key="边框颜色浅" Color="#FFFF99CC"/>
    <SolidColorBrush x:Key="边框颜色" Color="#FFCC0033"/>
    <SolidColorBrush x:Key="字体颜色浅" Color="#FFFF99CC"/>
    <SolidColorBrush x:Key="字体颜色标准" Color="#FF101010"/>
    <SolidColorBrush x:Key="屏幕颜色" Color="#FF666666"/>
    <SolidColorBrush x:Key="表行颜色" Color="#FFFFCCCC" Opacity="0.8"/>
    <SolidColorBrush x:Key="表行颜色1" Color="#FFFF9999" Opacity="0.8"/>
    <SolidColorBrush x:Key="工作状态颜色" Color="#FFFF6666"/>
    <LinearGradientBrush x:Key="线性渐变颜色"
        EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFFF99CC"
                                      Offset="0"/>
                                        <GradientStop Color="#FFFFCCCC" 
                                      Offset="1"/>
                                    </LinearGradientBrush>
    <SolidColorBrush x:Key="控件有效" Color="#FFFF9999"/>
    <SolidColorBrush x:Key="窗口背景深色半透明"
                     Color="#FFFF99CC" Opacity="0.6"/>
</ResourceDictionary>