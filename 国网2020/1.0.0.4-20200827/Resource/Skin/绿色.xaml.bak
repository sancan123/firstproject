<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Control colors.-->
    <SolidColorBrush x:Key="窗口背景色"
                     Color="#FFB9ECB9"/>
    <!--Border colors-->    
    <SolidColorBrush x:Key="窗口背景深色" Color="#FF99CC99" /> 
    <SolidColorBrush x:Key="分隔条颜色"
                     Color="#FF99CC99"/>
    <SolidColorBrush x:Key="边框颜色浅" Color="White"/>
    <SolidColorBrush x:Key="边框颜色" Color="#FF669933"/>
    <SolidColorBrush x:Key="字体颜色浅" Color="#FF333366"/>
    <SolidColorBrush x:Key="字体颜色标准" Color="#FF333333"/>
    <SolidColorBrush x:Key="屏幕颜色" Color="#FF666666"/>
    <SolidColorBrush x:Key="表行颜色" Color="#FFCCFFCC"/>
    <SolidColorBrush x:Key="表行颜色1" Color="#FFB9ECB9"/>
    <SolidColorBrush x:Key="工作状态颜色" Color="#FF99CC99"/>  
                     <LinearGradientBrush x:Key="线性渐变颜色"
        EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFB9ECB9"
                                      Offset="0"/>
                                        <GradientStop Color="#FF99CC99" 
                                      Offset="1"/>
                                    </LinearGradientBrush>
    <SolidColorBrush x:Key="控件有效" Color="#FFB9ECB9"/>
    <SolidColorBrush x:Key="窗口背景深色半透明"
                     Color="#FF99CC99" Opacity="0.6"/>
</ResourceDictionary>